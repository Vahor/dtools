{
  "build": {
    "beforeDevCommand": "bun run dev",
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": []
  },
  "productName": "dtools",
  "version": "0.0.0",
  "identifier": "fr.vahor.dtools",
  "plugins": {},
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "dtools",
        "hiddenTitle": true,
        "fullscreen": false,
        "width": 800,
        "height": 600,
        "alwaysOnTop": true
      }
    ]
  }
}
